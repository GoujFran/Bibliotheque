<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ensai.bibliotheque">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EcranAccueil">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PackageFilm.EcranFilm" />
        <activity android:name=".PackageFilm.FicheFilm" />
        <activity android:name=".PackageFilm.EcranRechercheFilm" />
        <activity android:name=".PackageSerie.EcranSerie" />
        <activity android:name=".PackageSerie.FicheSerie" />
        <activity android:name=".PackageSerie.EcranRechercheSerie" />
        <activity android:name=".PackageSerie.EcranSaison" />
        <activity android:name=".PackageSerie.FicheSaison" />
        <activity android:name=".PackageSerie.FicheEpisode"></activity>
    </application>

</manifest>
